spark: /usr/local/spark/spark/bin/spark-submit
infile: data/single_cell_samples.tsv
outfolder: data
meta: data/meta_cols.tsv
features: data/feature_cols.tsv
dimension_reduction: pca
outliers: mahalanobis
clustering: gmm
regression: glm
family: binomial
response: infection
n_components: 5
max_centers: 10
sparkparams:
  - "--driver-memory=3G"
  - "--executor-memory=6G"
debug: true
