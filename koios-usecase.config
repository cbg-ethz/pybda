spark: /usr/local/spark/spark/bin/spark-submit
infile: data/single_cell_samples.tsv
meta: ["cells", "perinuclei", "nuclei"]
features: ["cells", "perinuclei", "nuclei"]
response: "is_infected"
outfolder: data
family: binomial
dimension_reduction: factor_analysis
clustering: kmeans
n_components: 15
max_centers: 10
sparkparams:
  - "--driver-memory=3G"
  - "--executor-memory=6G"
