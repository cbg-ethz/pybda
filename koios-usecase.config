spark: /usr/local/spark/spark/bin/spark-submit
infile: data/single_cell_samples.tsv
outfolder: data
meta: data/meta_columns.tsv
features: data/feature_columns.tsv
dimension_reduction: factor_analysis
outliers: mahalanobis
clustering: kmeans
regression: glm
family: gaussian
n_components: 15
max_centers: 10
sparkparams:
  - "--driver-memory=3G"
  - "--executor-memory=6G"
